{
  "expo": {
    "name": "okemwa-playground",
    "slug": "okemwa-playground",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.okemwa-saf.okemwa-playground"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.okemwasaf.okemwaplayground"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-quick-actions",
        {
          "androidIcons": {
            "shortcut_one": {
              "foregroundImage": "./assets/images/adaptive-icon.png",
              "backgroundColor": "#282A35"
            },
            "shortcut_two": {
              "foregroundImage": "./assets/images/adaptive-star.png",
              "backgroundColor": "#ff0000"
            },
            "shortcut_three": "https://evanbacon.dev/pfp.png"
          },
          "iosIcons": {
            "shortcut_one": "./assets/images/adaptive-icon.png",
            "shortcut_two": {
              "1x": "./assets/images/adaptive-icon.png"
            }
          },
          "iosActions": [
            {
              "id": "1",
              "title": "Shortcut One",
              "subtitle": "Subtitle One",
              "icon": "shortcut_one",
              "params": {
                "href": "https://evanbacon.dev"
              }
            },
            {
              "id": "2",
              "title": "Shortcut Two",
              "subtitle": "Subtitle Two",
              "icon": "shortcut_one",
              "params": {
                "href": "https://evanbacon.dev"
              }
            }
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e5c0030d-09bd-4347-8c11-56d47c341eb5"
      }
    }
  }
}
